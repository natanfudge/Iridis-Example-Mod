{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",

    "name": "${display_name}",
    "description": "${description}",
    "authors": [
        "Me!"
    ],
    "contact": {
        "homepage": "${home_page}",
        "sources": "${sources}",
        "issues": "${issue_tracker}"
    },

    "license": "${license}",
    "icon": "${logo}",

    "environment": "*",
    "entrypoints": {
        "main": [
            "example.ExampleFabricMod"
        ]
    },

    "depends": {
        "fabricloader": ">=0.4.0",
        "fabric-api-base": "*",
        "fabric-events-lifecycle-v0": "*",
        "fabric-events-interaction-v0": "*",
        "fabric-networking-v0": "*"
    },
    "mixins": [
        "ladder-example-mod.mixins.json"
    ]
}